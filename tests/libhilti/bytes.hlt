#
# @TEST-EXEC:  hilti-build %INPUT -o a.out
# @TEST-EXEC:  ./a.out >output 2>&1
# @TEST-EXEC:  btest-diff output
#
module Main

import Hilti

void run() {
     local ref<bytes> b
     local int<64> i
     local bool x

     b = call Hilti::bytes_to_lower(b"AbCdEfG")
     call Hilti::print(b)

     b = call Hilti::bytes_to_lower(b"")
     call Hilti::print(b)

     ###

     i = call Hilti::bytes_to_int(b"1234")
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"1")
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"-1234")
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"-1")
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"000")
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"001")
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"a", 16)
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"ff", 16)
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"1A", 16)
     call Hilti::print(i)

     ####

     x = call Hilti::bytes_starts_with(b"abcdef", b"abc")
     call Hilti::print(x)

     x = call Hilti::bytes_starts_with(b"abc", b"abc")
     call Hilti::print(x)

     x = call Hilti::bytes_starts_with(b"abc", b"")
     call Hilti::print(x)

     x = call Hilti::bytes_starts_with(b"", b"")
     call Hilti::print(x)

     x = call Hilti::bytes_starts_with(b"ab", b"abc")
     call Hilti::print(x)

     x = call Hilti::bytes_starts_with(b"abc", b"abd")
     call Hilti::print(x)

     x = call Hilti::bytes_starts_with(b"", b"abc")
     call Hilti::print(x)

}

