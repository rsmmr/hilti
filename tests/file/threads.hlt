# @TEST-EXEC:  hilti-build %INPUT -o a.out
# @TEST-EXEC:  ./a.out
# @TEST-EXEC:  sort <output.log >output.log.sorted
# @TEST-EXEC:  btest-diff output.log.sorted

module Main

import Hilti

void print1() {
    local ref<file> f
    f = new file
    file.open f "output.log" (Hilti::FileType::Text, Hilti::FileMode::Append, "utf8")
    file.write f "1"
    file.close f
    return.void
}

void print2() {
    local ref<file> f
    f = new file
    file.open f "output.log" (Hilti::FileType::Text, Hilti::FileMode::Append, "utf8")
    file.write f "2"
    file.close f
    return.void
}

void print3() {
    local ref<file> f
    f = new file
    file.open f "output.log" (Hilti::FileType::Text, Hilti::FileMode::Append, "utf8")
    file.write f "3"
    file.close f
    return.void
}

void print4() {
    local ref<file> f
    f = new file
    file.open f "output.log" (Hilti::FileType::Text, Hilti::FileMode::Append, "utf8")
    file.write f "4"
    file.close f
    return.void
}

void print5() {
    local ref<file> f
    f = new file
    file.open f "output.log" (Hilti::FileType::Text, Hilti::FileMode::Append, "utf8")
    file.write f "5"
    file.close f
    return.void
}

void run() {
    thread.schedule print1() 1
    thread.schedule print2() 2
    thread.schedule print3() 3
    thread.schedule print4() 4
    thread.schedule print5() 5
}

