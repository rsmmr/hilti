module HTTP;

const LINE = /[^-\n]+\n/;
const DASHES = /---\n/;

type Item = unit {
    line:   LINE;
};

export type Test = unit {
    start: LINE;
    lines: list<Item>;
    : DASHES;
    last: LINE
        { print self.last; }
    };

