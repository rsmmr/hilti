#
# @TEST-EXEC:  echo 1234567890 | pac-driver-test %INPUT >output    
# @TEST-EXEC:  echo 2234567890 | pac-driver-test %INPUT >>output
# @TEST-EXEC:  echo 3234567890 | pac-driver-test %INPUT >>output
# @TEST-EXEC:  echo 4234567890 | pac-driver-test %INPUT >>output
# @TEST-EXEC:  btest-diff output
#

module Mini;

export type test = unit {

       a: bytes &length=1;

       switch ( self.a ) {
           b"1"       -> b1: bytes &length=4 &default=b"<not set>";
           b"2", b"3" -> b2: bytes &length=4 &default=b"<not set>";
           *          -> b3: bytes &length=4 &default=b"<not set>";
           };

       c: bytes &length=5
           {
           print "b1", self.b1;
           print "b2", self.b2;
           print "b3", self.b3;
           }
};
