module Mini;

export type Main = unit {
    a: bytes &length=5 -> self.data;
    b: bytes &length=55 -> self.data;

    var data: sink;

    on %init {
        self.data.connect(new Sub);
    }
};

export type Sub = unit {
    s1: bytes &length=55;
    on %done {
        print "Sub ", self;
    }
};

